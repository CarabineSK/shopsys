<div class="search" id="js-search-autocomplete">
	<form action="{{ url('front_product_search') }}" method="get">
		<input
			type="text"
			name="{{ constant('SS6\\ShopBundle\\Controller\\Front\\ProductController::SEARCH_TEXT_PARAMETER') }}"
			value="{{ searchText|escape('html_attr') }}"
			placeholder="{{ 'Vyhledávání'|trans }}"
			class="form-control search__input"
			id="js-search-autocomplete-input"
			autocomplete="off"
			data-autocomplete-url="{{ url('front_search_autocomplete') }}"
		/>

		<div class="search__block"></div>

		<div id="js-search-autocomplete-results"></div>
	</form>
</div>