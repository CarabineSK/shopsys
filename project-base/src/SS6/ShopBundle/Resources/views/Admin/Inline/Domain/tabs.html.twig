{% if domainConfigs|length > 1 %}
	<div class="box-domain" role="tablist">
		{% for domainConfig in domainConfigs %}
			{% if domainConfig.id == selectedDomainId %}
				<span class="box-domain__item box-domain__item--active">
					{{ domainIcon(domainConfig.id) }}
					{{ domainConfig.name }}
				</span>
			{% else %}
				<a class="box-domain__item" href="{{ url('admin_domain_selectdomain', {id: domainConfig.id}) }}">
					{{ domainIcon(domainConfig.id) }}
					{{ domainConfig.name }}
				</a>
			{% endif %}
		{% endfor %}
	</div>
{% endif %}