{% extends '@SS6Shop/Admin/Grid/PKGrid.html.twig' %}

{% block pkgrid_no_data %}
	Nebylo nalezeno žádné zboží. Nejdřív musíte nějaké vytvořit.
{% endblock %}

{% block pkgrid_value_cell_id_name %}
	<a href="{{ url('admin_product_edit', { id: row.id }) }}">{{ value }}</a>
{% endblock %}

{% block pkgrid_value_cell_id_price %}
	{{ value|price }}
{% endblock %}

{% block pkgrid_pager_totalcount %}
	{% set entityName = 'zboží' %}
	{{ parent() }}
{% endblock %}
