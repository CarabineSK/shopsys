{
	"name": "shopsys/shopsys",
	"type": "project",
	"description": "Copyright by netdevelo s.r.o.",
	"version": "6.0.0-dev",
	"autoload": {
		"psr-4": {
			"SS6\\": [
				"app/",
				"src/SS6/"
			]
		},
		"files": [ "src/SS6/ShopBundle/Component/Translation/functions.php" ]
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/doctrine2.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/doctrine-translatable-bundle.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/jparser.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/JMSTranslationBundle.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/Routing.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/coding-standards.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/phpstorm-inspect.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopsys/migrations.git"
		}
	],
	"require": {
		"php": "5.6.*",
		"arvenil/ninja-mutex": "0.4.1",
		"bmatzner/jquery-bundle": "1.11.1",
		"bmatzner/jquery-ui-bundle": "1.10.3",
		"commerceguys/intl": "0.7",
		"craue/formflow-bundle": "dev-master#d14fcb1",
		"doctrine/common": "2.5.0",
		"doctrine/doctrine-bundle": "1.6.1",
		"doctrine/doctrine-fixtures-bundle": "2.3.0",
		"doctrine/orm": "dev-ddc1960-hotfix-and-ddc4005-hotfix as 2.5.1",
		"egeloen/ckeditor-bundle": "4.0.1",
		"fp/jsformvalidator-bundle": "dev-master#4a12964",
		"fzaninotto/faker": "1.5.0",
		"helios-ag/fm-elfinder-bundle": "4.0.2",
		"incenteev/composer-parameter-handler": "2.1.2",
		"intaro/postgres-search-bundle": "dev-master#0eb26e0812",
		"intervention/image": "2.3.5",
		"jms/translation-bundle": "dev-fix-external-translations#527da12",
		"phing/phing": "2.13.0",
		"phpunit/phpunit": "4.8.27",
		"presta/sitemap-bundle": "1.4.2",
		"prezent/doctrine-translatable-bundle": "dev-rise-locale-listener-priority#09863c2",
		"raulfraile/ladybug-bundle": "1.0.6",
		"sensio/distribution-bundle": "4.0.1",
		"sensio/framework-extra-bundle": "3.0.10",
		"sensio/generator-bundle": "2.5.3",
		"shopsys/migrations": "1.3.0",
		"stof/doctrine-extensions-bundle": "1.2.1",
		"symfony/assetic-bundle": "2.6.1",
		"symfony/finder": "2.7.3",
		"symfony/monolog-bundle": "2.7.1",
		"symfony/swiftmailer-bundle": "2.3.8",
		"symfony/symfony": "2.7.3",
		"symfony-cmf/routing": "dev-master#bb25b51744",
		"symfony-cmf/routing-bundle": "1.3.3",
		"timwhitlock/jparser": "dev-get-rid-of-import#c37f4c6",
		"tracy/tracy": "2.3.8",
		"twig/extensions": "1.3.0"
	},
	"require-dev": {
		"codeception/codeception": "dev-master#865bfbd",
		"se/selenium-server-standalone": "2.53.0",
		"shopsys/coding-standards": "1.2.0",
		"shopsys/phpstorm-inspect": "1.0.4",
		"symfony/web-profiler-bundle": "2.8.2"
	},
	"scripts": {
		"post-install-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"SS6\\Environment::checkEnvironment"
		],
		"post-update-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"SS6\\Environment::checkEnvironment"
		]
	},
	"config": {
		"bin-dir": "bin"
	},
	"extra": {
		"symfony-app-dir": "app",
		"symfony-web-dir": "web",
		"incenteev-parameters": [
			{ "file": "app/config/parameters.yml" },
			{ "file": "app/config/parameters_test.yml" }
		]
	}
}
