{
	"name": "Shopsys6",
	"type": "project",
	"description": "Copyright by netdevelo s.r.o.",
	"version": "6.0.0-dev",
	"autoload": {
		"psr-4": {
			"SS6\\": [
				"app/",
				"src/SS6/"
			]
		},
		"files": [ "src/SS6/ShopBundle/Component/Translation/functions.php" ]
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/sustmi/jparser.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/sustmi/JsFormValidatorBundle.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/sustmi/JMSTranslationBundle.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/sustmi/Routing.git"
		},
		{
			"type": "vcs",
			"url": "http://gitlab.netdevelo/ss6/phpstorm-inspect.git"
		}
	],
	"require": {
		"php": "5.6.*",
		"arvenil/ninja-mutex": "0.4.1",
		"bmatzner/jquery-bundle": "1.11.1",
		"bmatzner/jquery-ui-bundle": "1.10.3",
		"commerceguys/intl": "0.5",
		"craue/formflow-bundle": "dev-master#55da501",
		"doctrine/common": "2.5.0",
		"doctrine/doctrine-bundle": "1.5.0",
		"doctrine/doctrine-fixtures-bundle": "2.2.0",
		"doctrine/doctrine-migrations-bundle": "v1.1.0",
		"doctrine/orm": "2.5.0",
		"egeloen/ckeditor-bundle": "2.5.0",
		"fabpot/php-cs-fixer": "1.8.1",
		"fp/jsformvalidator-bundle": "dev-translate-invalid-message#bb2be7e",
		"helios-ag/fm-elfinder-bundle": "4.0.2",
		"incenteev/composer-parameter-handler": "2.1.1",
		"intaro/postgres-search-bundle": "dev-master#0eb26e0812",
		"intervention/image": "2.0.17",
		"jms/translation-bundle": "dev-fix-external-translations#527da12",
		"pdepend/pdepend": "2.0.0",
		"phing/phing": "2.9.1",
		"phpmd/phpmd": "2.0.0",
		"phpunit/phpunit": "4.8.4",
		"presta/sitemap-bundle": "1.4",
		"prezent/doctrine-translatable-bundle": "dev-master#adfc849",
		"raulfraile/ladybug-bundle": "1.0.6",
		"sensio/distribution-bundle": "4.0.1",
		"sensio/framework-extra-bundle": "3.0.10",
		"sensio/generator-bundle": "2.5.3",
		"stof/doctrine-extensions-bundle": "1.2.1",
		"symfony/assetic-bundle": "2.6.1",
		"symfony/finder": "2.7.3",
		"symfony/monolog-bundle": "2.7.1",
		"symfony/swiftmailer-bundle": "2.3.8",
		"symfony/symfony": "2.7.3",
		"symfony-cmf/routing": "dev-master#bb25b51744",
		"symfony-cmf/routing-bundle": "1.3.3",
		"timwhitlock/jparser": "dev-fix-imports#e996c9c",
		"tracy/tracy": "2.2.6",
		"twig/extensions": "1.2.0"
	},
	"require-dev": {
		"codeception/codeception": "2.1.2",
		"jakub-onderka/php-parallel-lint": "dev-master#41365ce",
		"se/selenium-server-standalone": "2.45.0",
		"shopsys/phpstorm-inspect": "1.0.4",
		"squizlabs/php_codesniffer": "2.3.2"
	},
	"scripts": {
		"post-install-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"SS6\\Environment::checkEnvironment"
		],
		"post-update-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"SS6\\Environment::checkEnvironment"
		]
	},
	"config": {
		"bin-dir": "bin"
	},
	"extra": {
		"symfony-app-dir": "app",
		"symfony-web-dir": "web",
		"incenteev-parameters": [
			{ "file": "app/config/parameters.yml" },
			{ "file": "app/config/parameters_test.yml" }
		]
	}
}
