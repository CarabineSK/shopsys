{
	"name": "Shopsys6",
	"type": "project",
	"description": "Copyright by netdevelo s.r.o.",
	"version": "6.0.0-dev",
	"autoload": {
		"classmap": ["src/"],
		"psr-0": {"SS6\\": "src/"},
		"psr-4": {"SS6\\": "app/"}
	},
	"require": {
		"php": ">=5.4",
		"craue/formflow-bundle": "3.0.*@dev",
		"doctrine/orm": "~2.2,>=2.2.3",
		"doctrine/doctrine-bundle": "~1.2",
		"doctrine/doctrine-fixtures-bundle": "2.2.*",
		"egeloen/ckeditor-bundle": "2.4.*@dev",
		"helios-ag/fm-elfinder-bundle": "1.x",
		"incenteev/composer-parameter-handler": "~2.0",
		"jakub-onderka/php-parallel-lint": "0.7",
		"jms/translation-bundle": "1.1.0",
		"kutny/autowiring-bundle": "1.0",
		"pdepend/pdepend": "2.0.0.*@dev",
		"phing/phing": "2.8.2",
		"phpmd/phpmd": "2.0.0.*@dev",
		"phpunit/phpunit": "4.1.*",
		"raulfraile/ladybug-bundle": "~1.0",
		"sensio/distribution-bundle": "~3.0",
		"sensio/framework-extra-bundle": "~3.0",
		"sensio/generator-bundle": "~2.3",
		"squizlabs/php_codesniffer": "2.0.*@dev",
		"symfony/assetic-bundle": "~2.3",
		"symfony/dependency-injection": "~2.4",
		"symfony/monolog-bundle": "~2.4",
		"symfony/swiftmailer-bundle": "~2.3",
		"symfony/symfony": "2.5.*",
		"twig/extensions": "~1.0"
	},
	"require-dev": {},
	"scripts": {
		"post-install-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"SS6\\Environment::checkEnvironment"
		],
		"post-update-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"SS6\\Environment::checkEnvironment"
		]
	},
	"config": {
		"bin-dir": "bin"
	},
	"extra": {
		"symfony-app-dir": "app",
		"symfony-web-dir": "web",
		"incenteev-parameters": [
			{ "file": "app/config/parameters.yml" },
			{ "file": "app/config/parameters_test.yml" }
		]
	}
}
