{
	"name": "Shopsys6",
	"type": "project",
	"description": "Copyright by netdevelo s.r.o.",
	"version": "6.0.0-dev",
	"autoload": {
		"classmap": ["src/"],
		"psr-0": {"SS6\\": "src/"},
		"psr-4": {"SS6\\": "app/"}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/sustmi/jparser.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/sustmi/JsFormValidatorBundle.git"
		}
	],
	"require": {
		"php": ">=5.6",
		"bmatzner/jquery-bundle": "1.11.1",
		"bmatzner/jquery-ui-bundle": "1.10.*",
		"craue/formflow-bundle": "3.0.*@dev",
		"doctrine/common": "2.4.x-dev#434c364b6a8780c16769b60fcc56e61e6bac766f",
		"doctrine/doctrine-bundle": "~1.2",
		"doctrine/doctrine-fixtures-bundle": "2.2.*",
		"doctrine/orm": "~2.4,>=2.4.6",
		"egeloen/ckeditor-bundle": "2.5.*",
		"fabpot/php-cs-fixer": "1.3",
		"fp/jsformvalidator-bundle": "dev-translate-invalid-message",
		"helios-ag/fm-elfinder-bundle": "1.x",
		"incenteev/composer-parameter-handler": "~2.0",
		"intervention/image": "2.0.17",
		"jms/translation-bundle": "dev-master",
		"phing/phing": "2.9.1",
		"phpunit/phpunit": "4.1.*",
		"prezent/doctrine-translatable-bundle": "dev-master",
		"raulfraile/ladybug-bundle": "~1.0",
		"sensio/distribution-bundle": "~3.0",
		"sensio/framework-extra-bundle": "~3.0",
		"sensio/generator-bundle": "~2.3",
		"stof/doctrine-extensions-bundle": "1.1.0",
		"symfony/assetic-bundle": "~2.6",
		"symfony/monolog-bundle": "~2.7",
		"symfony/swiftmailer-bundle": "~2.3",
		"symfony/symfony": "2.6.4",
		"symfony-cmf/routing-bundle": "~1.3",
		"timwhitlock/jparser": "dev-fix-imports",
		"tracy/tracy": "2.2.6",
		"twig/extensions": "~1.0"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "dev-master",
		"pdepend/pdepend": "2.0.0.*@dev",
		"phpmd/phpmd": "2.0.0.*@dev",
		"squizlabs/php_codesniffer": "2.0.*@dev"
	},
	"scripts": {
		"post-install-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"SS6\\Environment::checkEnvironment"
		],
		"post-update-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"SS6\\Environment::checkEnvironment"
		]
	},
	"config": {
		"bin-dir": "bin"
	},
	"extra": {
		"symfony-app-dir": "app",
		"symfony-web-dir": "web",
		"incenteev-parameters": [
			{ "file": "app/config/parameters.yml" },
			{ "file": "app/config/parameters_test.yml" }
		]
	}
}
